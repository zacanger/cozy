let fb = fn(n) {
    if (n % 15 == 0) { return "Fizzbuzz"; }
    if (n % 3 == 0) { return "Fizz"; }
    if (n % 5 == 0) { return "Buzz"; }
    # implicit return
    string(n);
};

let fizzbuzz = fn(n = 100) {
    mutable ret = [];

    foreach i in 0..n {
        ret = ret.push(fb(i));
    }

    return ret.join("\n");
};

print(fizzbuzz(10));
